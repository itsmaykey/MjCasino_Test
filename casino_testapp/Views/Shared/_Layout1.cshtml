<body ng-app="MJCasinoapp">
    <!-- ======= Header ======= -->
    <header id="header" class="header fixed-top d-flex align-items-center" style="background-color: #29303A;">


        <div class="d-flex align-items-center justify-content-between">
            <a href="~/Dashboard/index" class="logo d-flex align-items-center">
                <img src="~/Content/assets/img/logo.png" alt="">
                @* <span class="d-none d-lg-block" style="color:beige;">Mj Casino</span> *@
            </a>
            <i class="bi bi-list toggle-sidebar-btn" id="toggle" style="color: beige;"> <span></span></i>
        </div><!-- End Logo -->
        <div class="search-bar" style="display: hidden;">

        </div>
        <!-- End Search Bar -->
        <nav id="header" class="header-nav ms-auto">
            <div id="logged" style="display: none;">
                <ul class="d-flex align-items-center">
                    <!-- End Search Icon-->
                    <li class="nav-item dropdown pe-3">
                        <div class="nav-profile">
                            <form class="search-form d-flex align-items-center" style="justify-content: flex-end;"
                                method="POST" action="#">
                                <input type="text" name="query" placeholder="Php 0.00" title="Enter search keyword"
                                    style="width:fit-content; background:beige;" disabled>
                                <button type="submit" title="cash"><i class="bi bi-cash-coin"></i></button>
                            </form>
                        </div>
                    </li>
                    <li class="nav-item dropdown pe-3">
                        <a class="nav-link nav-profile d-flex align-items-center pe-0" href="#"
                            data-bs-toggle="dropdown">
                            <img src="~/Content/assets/img/account_avatar.png" alt="Profile" class="rounded-circle">

                        </a><!-- End Profile Iamge Icon -->
                        <ul class="dropdown-menu dropdown-menu-end dropdown-menu-arrow profile">
                            <li class="dropdown-header">
                                <h6 id="userName">{{username}}</h6>

                            </li>
                            <li>
                                <hr class="dropdown-divider">
                            </li>

                            <li>
                                <a class="dropdown-item d-flex align-items-center" href="~/Profile/Account">
                                    <i class="bi bi-person"></i>
                                    <span>My Profile</span>
                                </a>
                            </li>
                            <li>
                                <hr class="dropdown-divider">
                            </li>

                            <li>
                                <a class="dropdown-item d-flex align-items-center" href="users-profile.html">
                                    <i class="bi bi-gear"></i>
                                    <span>Account Settings</span>
                                </a>
                            </li>
                            <li>
                                <hr class="dropdown-divider">
                            </li>

                            <li>
                                <a class="dropdown-item d-flex align-items-center" href="pages-faq.html">
                                    <i class="bi bi-question-circle"></i>
                                    <span>Need Help?</span>
                                </a>
                            </li>
                            <li>
                                <hr class="dropdown-divider">
                            </li>

                            <li>
                                <a class="dropdown-item d-flex align-items-center" href="#">
                                    <i class="bi bi-box-arrow-right"></i>
                                    <span>Sign Out</span>
                                </a>
                            </li>

                        </ul><!-- End Profile Dropdown Items -->
                    </li><!-- End Profile Nav -->

                </ul>
            </div>

            <div id="notlogged" style="display: none; margin-right: 15px;">
                <button type="button" class="btn btn-success" onclick="toLogin()">Login</button>
                <button type="button" class="btn btn-success" onclick="toRegister()">Register</button>
            </div>




        </nav>

        <!-- End Icons Navigation -->

    </header><!-- End Header -->
    <!-- ======= Sidebar ======= -->
    <script type="text/javascript">

       
        var userData_obj = sessionStorage.getItem('user');
        var userData = JSON.parse(userData_obj);
        if (userData != null) {
            // username = userData.data['username'];
            console.log(userData);
            const u = {
                username: userData.data['username']
            }
            console.log(u);
            w3.displayObject("userName", u);
            w3.show("logged");
            var logged = document.getElementById("logged");
            if (logged.style.display === "none") {
                logged.style.display = "block";
            }

        } else {
            var notlogged = document.getElementById("notlogged");
            if (notlogged.style.display === "none") {
                notlogged.style.display = "block";
            }
        }
        function toLogin() {
            window.open("../Authorization/login", '_self');

        }
        function toRegister() {
            window.open("../Authorization/Registration", '_self');
        }



    </script>
    <aside id="sidebar" class="sidebar" style="background-color: #29303A;">

        <ul class="sidebar-nav" id="sidebar-nav">

            <li class="nav-item">
                <a class="nav-link collapsed" href="~/Dashboard/index">
                    <i class="bi bi-grid"></i>
                    <span>Dashboard</span>
                </a>
            </li><!-- End Dashboard Nav -->

            <li class="nav-heading">Pages</li>
            <li class="nav-item">
                <a class="nav-link collapsed" href="~/Authorization/Registration">
                    <i class=" bi bi-card-list">
                    </i>
                    <span>Register</span>
                </a>
            </li><!-- End Register Page Nav -->

            <li class="nav-item">
                <a class="nav-link collapsed" href="~/Authorization/Login">
                    <i class="bi bi-box-arrow-in-right"></i>
                    <span>Login</span>
                </a>
            </li><!-- End Login Page Nav -->


        </ul>

    </aside><!-- End Sidebar-->

    <main id="main" class="main layoutbg">
        @*<section class="section">*@
        <div class="content-wrapper">
            @RenderBody()
