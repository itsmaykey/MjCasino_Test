@{
    ViewBag.Title = "Dashboard";

}

<meta name="viewport" content="width=device-width, initial-scale=1">
<style>
    .customstyle {
        display: flex;
        justify-content: space-between;
    }

    .img {
        display: inline;
        max-width: 100%;
        height: auto;
        /* ie8 */
    }
        .forcenter {
            text-align: center;
            align-items: center;
            justify-content: center;
        }

        .parent-element {
            text-align: center;
        }

        .parent-element ul {
            list-style-type: none;
            margin: 0;
            padding: 0;
        }

        .parent-element ul li {
            margin: 0;
            padding: 0;
        }
    
</style>

<div ng-controller="dashboardCtrlr">
    <div class="nav navbars">
        <ul class="nav-tabs nav-tabs-bordered d-flex navbuttons" id="borderedTabJustified" role="tablist">
            <li class="nav-item flex-fill" role="presentation">
                <button class="nav-link w-45 active" id="home-tab" data-bs-toggle="tab"
                        data-bs-target="#bordered-justified-home" type="button" role="tab" aria-controls="home"
                        aria-selected="true">
                    <img src="~/Content/assets/img/Dashboard/Home.png" alt="">
                </button>
            </li>
            <li class="nav-item flex-fill" role="presentation">
                <button class="nav-link w-45" id="home-tab" data-bs-toggle="tab"
                        data-bs-target="#bordered-justified-home" type="button" role="tab" aria-controls="home"
                        aria-selected="true">
                    <img src="~/Content/assets/img/Dashboard/Slotmachine.png" alt="">
                </button>
            </li>
            <li class="nav-item flex-fill" role="presentation">
                <button class="nav-link w-45" id="home-tab" data-bs-toggle="tab"
                        data-bs-target="#bordered-justified-home" type="button" role="tab" aria-controls="home"
                        aria-selected="true">
                    <img src="~/Content/assets/img/Dashboard/CARDs.png" alt="">
                </button>
            </li>
            <li class="nav-item flex-fill" role="presentation">
                <button class="nav-link w-45" id="home-tab" data-bs-toggle="tab"
