
@{
    ViewBag.Title = "_Deposit";
}


<div ng-controller="layoutCtrl">

    <table class="table table-dark">
        <thead>
            <tr>
                <th scope="col">username</th>
                <th scope="col">tag</th>
            </tr>
            {{userBalance}}
        </thead>
        <tbody ng-repeat="x in userDownlines">
            <tr>
                <td>{{x.balance_username}}</td>
                <td>{{x.balance_tag}}</td>
            </tr>

        </tbody>
    </table>


</div>
